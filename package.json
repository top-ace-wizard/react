{
  "name": "@dialectlabs/workspaces",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/dialectlabs/react"
  },
  "scripts": {
    "pkg": "manypkg run",
    "bootstrap": "bun build:all",
    "build:all": "bun run build:react-sdk && bun run build:react-sdk-solana && bun run build:react-ui",
    "build:react-ui": "bun pkg @dialectlabs/react-ui build",
    "dev:react-ui": "bun pkg @dialectlabs/react-ui dev",
    "build:react-sdk": "bun pkg @dialectlabs/react-sdk build",
    "dev:react-sdk": "bun pkg @dialectlabs/react-sdk dev",
    "build:react-sdk-solana": "bun pkg @dialectlabs/react-sdk-blockchain-solana build",
    "dev:react-sdk-solana": "bun pkg @dialectlabs/react-sdk-blockchain-solana dev",
    "lint": "eslint --ext .js --ext .ts --ext .tsx .",
    "lint:fix": "bun run lint --fix"
  },
  "devDependencies": {
    "@manypkg/cli": "^0.21.3",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "keywords": [],
  "author": "@dialectlabs",
  "license": "Apache-2.0",
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@dialectlabs/react-ui": "workspace:*"
  }
}
