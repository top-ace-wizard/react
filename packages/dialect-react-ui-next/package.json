{
  "name": "@dialectlabs/react-ui-next",
  "version": "2.0.0-next",
  "type": "module",
  "private": false,
  "sideEffects": false,
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./index.css": "./lib/index.css"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "dev": "bun clean && concurrently \"bun:build:cjs:watch\" \"bun:build:esm:watch\" \"bun:build:styles:watch\"",
    "build:cjs": "tsc",
    "build:cjs:watch": "tsc --watch",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:esm:watch": "tsc --project tsconfig.esm.json --watch",
    "build:styles": "bun tailwindcss -i index.css -o lib/index.css --minify",
    "build:styles:watch": "bun tailwindcss -i index.css -o lib/index.css --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^18.2.73",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dialectlabs/react"
  }
}